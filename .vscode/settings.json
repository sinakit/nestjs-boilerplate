{
    "editor.formatOnSave": true,
    "editor.tabSize": 2,
    "files.exclude": {
      "**/.git": true,
      "**/.svn": true,
      "**/.hg": true,
      "**/CVS": true,
      "**/.DS_Store": true,
      ".nyc_output": true,
      ".vscode": false,
      "coverage": true,
      "dist": true,
      "node_modules": true
    },
    "git.autofetch": true,
    "typescriptHero.imports.multiLineTrailingComma": false,
    "typescriptHero.imports.multiLineWrapThreshold": 120,
    "typescriptHero.imports.removeTrailingIndex": true,
    "typescript.tsdk": "node_modules/typescript/lib",
    "typescriptHero.imports.grouping": [
      "Plains",
      "Modules",
      "/^@api|@common|@dynamodb|@entities|@lib|@models|@queries|@repositories|@schema/",
      "Workspace"
    ],
    "search.exclude": {
      "**/node_modules": true,
      "**/bower_components": true,
      "**/dist": true
    },
    "cSpell.words": [
      "apns",
      "authen",
      "backend",
      "bolditalics",
      "callsite",
      "Certbot",
      "cognito",
      "commitlint",
      "dayoff",
      "dynamodb",
      "elasticbeanstalk",
      "gmail",
      "homenet",
      "Injectable",
      "keycloak",
      "ldap",
      "Metacounter",
      "middlewares",
      "mimetype",
      "nestjs",
      "Nginx",
      "originalname",
      "otps",
      "pathmazing",
      "pdfmake",
      "plasgate",
      "promisify",
      "QWIQ",
      "qwiqlocation",
      "rabbitmq",
      "randomatic",
      "sendbird",
      "systemctl",
      "timestamps",
      "Twilio",
      "Typegoose",
      "typeorm",
      "udid",
      "Unban",
      "Unbans",
      "undoable",
      "unmock",
      "Unmutes",
      "UNPROCESSABLE",
      "Unregisters",
      "upsert",
      "Urlencoding",
      "userid",
      "Validators",
      "varchar",
      "Webhook",
      "webhooks",
      "websockets",
      "Whitelisted",
      "wowza"
    ]
  }
  